    <script language="javascript" type="text/javascript">

      var myWidth = 800;
      var myHeight = 600;

      var showInfoTitle = '';

      $(document).ready(function() {
        var fromDate = new Date("October 30, 2010 00:00:01");
        var toDate   = new Date("November 1, 2010 00:00:01");
        var today    = new Date();

        if ( fromDate < today && today < toDate ) {
          document.getElementById('bibdk_logo_img').setAttribute('src', 'common/pics/10aar/logo/bibdk_10aar_logo.png'); // href ='common/pics/10aar/logo/bibdk_10aar_logo_2.png';
          document.getElementById('bibdk_logo_img').style.height='70px';
          document.getElementById('bibdk_logo_img').style.width='260px';
          var totalNum = 3;//the total number of images in a folder.
          var rndNum = Math.floor(Math.random() * totalNum) + 1;
          document.getElementById('hdr').style.backgroundImage  = "url(common/pics/10aar/banner/banner_10aar_" + rndNum  + ".jpg)";
        } else {
          var totalNum = 31;//the total number of images in a folder.
          var rndNum = Math.floor(Math.random() * totalNum);
          document.getElementById('hdr').style.backgroundImage  = "url(common/pics/topimages/" + rndNum  + ".jpg)";
        }
	
        if(getURLParam('wayf_show_help')==1) {
                showInfo(document.getElementById('logged_in_as'),'help_wayf_first_login');
        }
        if(getURLParam('wayf_init_logout')==1) {
		alert('');
        }


        FocusFirst();

      });

        CART_COOKIE_NAME = "bibdk_cart";
  
      function linkTo(url) {
        window.location.href = url;
      }

      function openOrCloseBlock(id,replace) {
       if ( arguments.length == 2 ) replace = false; else replace = true;

       var element = id;
       var linkSign = 'linkSign-' + id;
        if ( document.getElementById(element).style.display != 'block' ) {
          document.getElementById(element).style.display = 'block';
          if ( replace )
          {
              document.getElementById(linkSign).innerHTML = '[&divide;]';
          }
        } else{
          document.getElementById(element).style.display = 'none';
          if ( replace )
          {
              document.getElementById(linkSign).innerHTML = '[+]';
          }
        }
      }


    </script>


