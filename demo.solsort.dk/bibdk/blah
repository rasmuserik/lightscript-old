    <script language="javascript" type="text/javascript">

      var rec_owner=0;
      var local_id=0;
      var id=0;
      var newVal="0";

      function rateSubmit(no,ejer,idnr) {
        rec_owner=ejer;
        local_id=idnr;
        id=rec_owner+local_id;

      $.get("http://bibliotek.dk/star_rating_jquery_server.php",
          {val: no, rec_owner: rec_owner, local_id: local_id},
          function(returned_data)
             {
              $("#stars_"+rec_owner+local_id).addClass("stars"+returned_data);
              newVal=returned_data;
             });
           $("#ucd_num_raters_"+id).text('Tak for din vurdering!');
      }

      function setRatingVal(prevVal, id) {
        if(newVal>0)
          document.getElementById("stars_"+id).className='stars'+newVal;
        else
          document.getElementById("stars_"+id).className='stars'+prevVal;
      }
    </script>

